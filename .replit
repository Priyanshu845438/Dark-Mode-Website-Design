modules = ["web", "php-8.2", "nodejs-20"]
run = "node src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/main.js"

[nix]
channel = "stable-25_05"
packages = ["wget", "unzip"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Web Server"

[[workflows.workflow]]
name = "Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php -S 0.0.0.0:5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true

[deployment]
run = ["sh", "-c", "node src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/src/scripts/main.js"]
